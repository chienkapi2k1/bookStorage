                                        <li><a href="@Url.Action("PhanLoaiSach","Sach")"><i class="ri-function-line"></i>Danh mục mặt hàng</a></li>
                                        <li><a href="@Url.Action("GioHang","Sach")"><i class="ri-checkbox-multiple-blank-line"></i>Giỏ Hàng</a></li>
                                        <li><a href="@Url.Action("DSDonDat","Sach")"><i class="ri-checkbox-multiple-blank-line"></i>Danh sách đơn mua</a></li>
                                    </ul>
                                </li>
                            }
                            else
                            {
                                <li class="active active-menu">
                                    <a href="#dashboard" class="iq-waves-effect" data-toggle="collapse" aria-expanded="true"><span class="ripple rippleEffect"></span><i class="las la-home iq-arrow-left"></i><span>BookSto</span><i class="ri-arrow-right-s-line iq-arrow-right"></i></a>
                                    <ul id="dashboard" class="iq-submenu collapse show" data-parent="#iq-sidebar-toggle">
                                        <li class="active"><a href="@Url.Action("TrangChu","Sach")"><i class="las la-house-damage"></i>Trang chủ</a></li>
                                        <li><a href="@Url.Action("PhanLoaiSach","Sach")"><i class="ri-function-line"></i>Danh mục mặt hàng</a></li>
                                        <li><a href="@Url.Action("GioHang","Sach")"><i class="ri-checkbox-multiple-blank-line"></i>Giỏ Hàng</a></li>
                                        <li><a href="@Url.Action("DSDonDat","Sach")"><i class="ri-checkbox-multiple-blank-line"></i>Danh sách đơn mua</a></li>
                                    </ul>
                                </li>
                            }

                        }
                        else
                        {
                            <li class="active active-menu">
                                <a href="#dashboard" class="iq-waves-effect" data-toggle="collapse" aria-expanded="true"><span class="ripple rippleEffect"></span><i class="las la-home iq-arrow-left"></i><span>BookSto</span><i class="ri-arrow-right-s-line iq-arrow-right"></i></a>
