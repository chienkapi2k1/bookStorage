<!-- Mirrored from templates.iqonic.design/booksto/html/admin-dashboard.html by HTTrack Website Copier/3.x [XR&CO'2014], Wed, 26 Oct 2022 15:07:07 GMT -->
<head>
    <!-- Required meta tags -->
    <link rel="shortcut icon" href="~/template/booksto/html/images/favicon.ico" />
    <!-- Bootstrap CSS -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>@ViewBag.Title</title>
    <!-- Favicon -->
    <link rel="shortcut icon" href="images/favicon.ico" />
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="~/template/booksto/html/css/bootstrap.min.css">
    <!-- Typography CSS -->
    <link rel="stylesheet" href="~/template/booksto/html/css/typography.css">
    <!-- Style CSS -->
    <link rel="stylesheet" href="~/template/booksto/html/css/style.css">
