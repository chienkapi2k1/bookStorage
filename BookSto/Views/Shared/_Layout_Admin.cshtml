                                    <ul id="admin" class="iq-submenu collapse show" data-parent="#iq-sidebar-toggle">
                                        @if (Session["Role"].ToString() == "ADMIN")
                                        {
                                            <li class="active"><a href="@Url.Action("BangDieuKhien","Admin")"><i class="ri-dashboard-line"></i>Bảng điều khiển</a></li>
                                            <li><a href="@Url.Action("DSTaiKhoan","Admin")"><i class="fa fa-users"></i>Tài khoản</a></li>
                                        }
                                        <li><a href="@Url.Action("DSTheLoai","Admin")"><i class="ri-list-check-2"></i>Thể loại Sách</a></li>
                                        <li><a href="@Url.Action("DSTacGia","Admin")"><i class="ri-file-user-line"></i>Tác Giả</a></li>
                                        <li><a href="@Url.Action("DSNXB","Admin")"><i class="ri-list-check-2"></i>Nhà xuất bản</a></li>
                                        <li><a href="@Url.Action("DSSach","Admin")"><i class="ri-book-2-line"></i>Sách</a></li>
                                        <li><a href="@Url.Action("DSViTri","Admin")"><i data-icon="g" class="icon"></i>Vị Trí</a></li>
                                        <li><a href="@Url.Action("DSThung","Admin")"><i class="ri-dashboard-line"></i>Thùng Hàng</a></li>
                                        <li><a href="@Url.Action("PhieuNhapKho","Admin")"><i class="las la-file-alt iq-arrow-left"></i>Phiếu Nhập Kho</a></li>
                                        <li><a href="@Url.Action("DSDonXuat","Admin")"><i class="las la-file-alt iq-arrow-left"></i>Phiếu Xuất Kho</a></li>

                                    </ul>
                                </li>
                                <li>
                                    <a href="#dashboard" class="iq-waves-effect" data-toggle="collapse" aria-expanded="false"><span class="ripple rippleEffect"></span><i class="las la-home iq-arrow-left"></i><span>BookSto</span><i class="ri-arrow-right-s-line iq-arrow-right"></i></a>
                                    <ul id="dashboard" class="iq-submenu collapse" data-parent="#iq-sidebar-toggle">
